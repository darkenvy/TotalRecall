{
  "name": "preact",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node internals/server",
    "build": "webpack --config internals/webpack/prod.js",
    "build:dll": "node internals/server/dll-generate.js"
  },
  "keywords": [],
  "author": "Reno McKenzie",
  "license": "ISC",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "redux-saga",
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "jsx": true
      }
    },
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "react/jsx-filename-extension": 0,
      "arrow-parens": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/require-default-props": 0,
      "react/destructuring-assignment": 0,
      "operator-linebreak": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/prod.js"
        }
      }
    }
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      "@babel/typescript",
      "@babel/react",
      [
        "@babel/env",
        {
          "targets": {
            "browsers": "last 2 Chrome versions"
          }
        }
      ]
    ]
  },
  "dllPlugin": {
    "path": "node_modules/react-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": []
  },
  "dependencies": {
    "axios": "^0.18.0",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "react-cookies": "^0.1.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "reselect": "^3.0.1",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.51",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/preset-typescript": "^7.0.0-beta.51",
    "babel-loader": "^8.0.0-beta.4",
    "circular-dependency-plugin": "^4.4.0",
    "compression": "^1.7.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.11",
    "ejs-loader": "^0.3.1",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "glob": "^7.1.2",
    "history": "^4.7.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "react-router-redux": "^4.0.8",
    "redux-saga": "^0.16.0",
    "shelljs": "^0.8.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.14.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.22.2"
  }
}